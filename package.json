{
  "name": "ultimate-express-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "imports": {
    "#src/*": "./src/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix",
    "format": "biome format --write",
    "dev": "concurrently \"pnpm watch:compile\" \"pnpm watch:dev\"",
    "watch:compile": "tsc --watch",
    "watch:dev": "wait-on ./dist/server.js && node --watch ./dist/server.js",
    "prepare": "ts-patch install && ts-node build/env.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanurb/ultimate-express-typescript.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sanurb/ultimate-express-typescript/issues"
  },
  "homepage": "https://github.com/sanurb/ultimate-express-typescript#readme",
  "dependencies": {
    "@agentica/core": "^0.21.0",
    "@agentica/rpc": "^0.21.0",
    "@reflet/http": "1.0.0",
    "@samchon/openapi": "^4.2.0",
    "@types/compression": "1.7.5",
    "@types/response-time": "2.3.8",
    "compression": "1.8.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "install": "0.13.0",
    "openai": "^4.96.2",
    "response-time": "2.3.3",
    "tgrid": "^1.1.0",
    "tstl": "^3.0.0",
    "typia": "^9.1.1",
    "ultimate-express": "1.4.8",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@amiceli/vitest-cucumber": "4.7.0",
    "@biomejs/biome": "1.9.4",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "22.15.3",
    "@types/supertest": "6.0.3",
    "chokidar": "4.0.3",
    "concurrently": "9.1.2",
    "supertest": "7.1.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "~5.8.3",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.48.0",
    "vitest": "3.1.2",
    "vitest-mock-extended": "3.1.0",
    "wait-on": "8.0.3"
  },
  "packageManager": "pnpm@10.10.0"
}
